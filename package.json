  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "lint:check": "tslint 'src/**/*.ts*'",
    "start": "react-app-rewired start",
    "start:local": "cross-env REACT_APP_BUILD=local react-app-rewired start",
    "start:dev": "cross-env REACT_APP_BUILD=development react-app-rewired start",
    "build:prod": "cross-env REACT_APP_BUILD=production react-app-rewired build",
    "build:dev": "cross-env REACT_APP_BUILD=development react-app-rewired build",
    "build:qa": "cross-env REACT_APP_BUILD=qa react-app-rewired build",
    "build:stg": "cross-env REACT_APP_BUILD=staging react-app-rewired build",
    "test": "react-app-rewired test -u",
    "jest": "CI=true react-app-rewired test -u",
    "lint:fix": "tslint 'src/**/*.ts*' --fix",
    "mock": "json-server mock/db.js",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "dsm-storybook:publish": "dsm-storybook publish",
    "dsm-storybook:preview": "dsm-storybook preview",
    "merge-report": "mochawesome-merge cypress/reports/separate-reports/*.json > cypress/reports/full_report.json",
    "generate-report": "marge cypress/reports/full_report.json -o . --cdn",
    "cypress-regression": "cypress run --browser chrome",
    "cypress-local": "cypress run --spec 'cypress/integration/SmokeTest/*' --env configFile=local --browser chrome",
    "e2e": "cypress run --env configFile=local --browser chrome",
    "e2e:smoke:local": "cypress run --spec 'cypress/integration/SmokeTest/*' --env configFile=local --browser chrome",
    "serve:build": "serve --no-clipboard --listen ${PORT:-3000} build",
    "e2e:ci": "run-p --race serve:build e2e",
    "e2e:ci:smoke:local": "run-p --race serve:build e2e:smoke:local"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run jest"
    }
  },
  "lint-staged": {
    "*.ts*": [
      "npm run lint:check"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
